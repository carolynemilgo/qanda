<div class="container-fluid">

  <div class="row">
    <div class="col-md-12">
      <div class="col-md-2">
      </div>
      <div class="col-md-8" id="content">
        {{#each model.questions as |question|}}
          <table>
            <tr>
              <td class="tag"><img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcSf4rBk6ahqj4d6hwXUkc88Hl-bK3NoGz6NDbyjqvJ4YytuO9mv5g"></td>
